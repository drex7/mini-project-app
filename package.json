{
  "name": "nuxt-prisma",
  "version": "2.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "packageManager": "yarn@4.9.2",
  "scripts": {
    "dev": "nuxt dev",
    "prepare": "nuxt prepare",
    "prebuild": "nuxt prepare && yarn run prisma:generate",
    "build": "nuxt build && yarn run copy_prisma_engine",
    "start": "node .output/server/index.mjs",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "copy_prisma_engine": "mv ./generated/prisma/libquery_engine* .output/server/node_modules/@prisma/client/"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.832.0",
    "@aws-sdk/s3-request-presigner": "^3.832.0",
    "@prisma/client": "6.9.0",
    "@prisma/extension-accelerate": "^2.0.1",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^8.0.0",
    "nuxt": "3.17.5",
    "postcss": "^8.5.6"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.27.5",
    "@nuxtjs/eslint-config": "12.0.0",
    "@nuxtjs/eslint-module": "4.1.0",
    "@nuxtjs/tailwindcss": "^7.0.0-beta.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "5.4.1",
    "prettier": "3.5.3",
    "prisma": "6.9.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  }
}
